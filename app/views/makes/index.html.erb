<h1>Listing makes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Kbb</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @makes.each do |make| %>
  <tr>
    <td><%= make.name %></td>
    <td><%= make.kbb_id %></td>
    <td><%= link_to 'Show', make %></td>
    <td><%= link_to 'Edit', edit_make_path(make) %></td>
    <td><%= link_to 'Destroy', make, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

