<h1>Listing car_trims</h1>

<table>
  <tr>
    <th>Car model</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @car_trims.each do |car_trim| %>
  <tr>
    <td><%= car_trim.model_id %></td>
    <td><%= car_trim.name %></td>
    <td><%= link_to 'Show', car_trim %></td>
    <td><%= link_to 'Edit', edit_car_trim_path(car_trim) %></td>
    <td><%= link_to 'Destroy', car_trim, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

